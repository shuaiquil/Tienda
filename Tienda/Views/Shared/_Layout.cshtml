<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' type='text/css' href='/fonts/fonts.css'>
    <link rel="shortcut icon" type="image/png" href="/Content/img/logo-popper.png">
    <link href="https://fonts.googleapis.com/css2?family=Abel&display=swap" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
    <title>@ViewBag.Title | Popper Store</title>

    @Scripts.Render("~/bundles/modernizr")

    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }

    @RenderSection("AddToHeadAnotherWay", required: false)
</head>
<body>
    @Html.Partial("ViewPartialHeader")



    @RenderBody()


    @Html.Partial("ViewPartialFooter")

</body>
</html>
